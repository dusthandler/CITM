#include <stdio.h>
int main() {
	int n=3, suma=5, result=n+suma,cont=0;
	if (cont == 0) {
		printf("%i,", n);
		cont++;
	}
	
	for (suma; result < 100; suma = suma + 2) {
		if (cont > 1) {
			printf("%i,", result);
		}
			result = n + suma;
			n = result;
		cont++;
	}
	printf("\n");
	system("pause");
	return 0;
}
