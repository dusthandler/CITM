//Ejerfcicio 16

#include<stdio.h>
int main() {
	int f, g, c, n;
	printf("dime la cantidad de filas");
	scanf_s("%i", &f);
	printf("dime el grosor");
	scanf_s("%i", &g);
	for (f = 1; f <= n; f++) {
		for (c = 1; c <= n; c++) {
			printf("*");
		}
		printf("\n");
	}
	system("pause");
	return 0;
}
