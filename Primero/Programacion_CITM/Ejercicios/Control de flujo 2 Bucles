//Ejercicio 11
/*
#include<stdio.h>
int main() {
	int n, f, c;
	printf("cuantas filas de estrellas quieres?\n");
	scanf_s("%i", &n);
	for (f = 1; f <= n; f++) {
		printf("%i\t", f);
		for (c = 1; c <= f; c++) {
			printf("*");
		}
		printf("\n");
	}
	system("pause");
	return 0;
}
*/

//Ejercicio 12
/*
#include <stdio.h>
int main() {
	int n, c, x;
	printf("dame un numero\n");
	scanf_s("%i", &n);
	for (c = 1; c <= n; c++) {
		for (x = 1; x < n; x++) {
			printf("*");
		}
		if (c <= n) {
			printf("*");
		}
		else {
			printf("\n");
			c++;
		}
		printf("\n");
	}
	system("pause");
	return 0;
}
*/

