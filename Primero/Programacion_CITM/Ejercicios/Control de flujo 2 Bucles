//Ejercicio 6
/*
#include<stdio.h>
int main() {
	int x, y, cont = 0;
	printf("Hasta que numero quieres buscar los impares?\n");
	scanf_s("%i", &y);
	system("cls");
	printf("tabla de los numeros impares menores que %i\n", y);
	for (x = 1; x <= y; x++) {
		if (x % 2 == 1) {
			printf("%i\t", x);
			cont++;
		}
		else if (cont % 5 == 0) {
			printf("\n");
		}
		else if (x == y) {
			printf("\n");
		}
	}
	system("pause");
	return 0;
}
*/

