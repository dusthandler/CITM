//Ejercicio 14 
/*
#include<stdio.h>
int main() {
	int f, n, c;
	printf("dame un numero\n");
	scanf_s("%i", &n);
	for (f = 1; f <= n; f++) {
		for (c = 1; c <= f; c++) {
			printf("*");
		}
		printf("\n");
	}
	for (f = n - 1; f >= 1; f--) {
		for (c = f; c >= 1; c--) {
			printf("*");
		}
		printf("\n");
	}
	system("pause");
	return 0;
}
*/

